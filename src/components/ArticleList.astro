---
export interface Props {
  title: any;
}

const { title } = Astro.props;
---

<style lang="scss">
  .wrapper {
    padding: 0 10px;
    max-width: 700px;
    margin: 0 auto;
    padding-top: 50px;
  }
  h1 {
    margin-bottom: 40px;
  }
</style>

<div class="wrapper">
  <h1>{title}</h1>

  <div class="list">
    <slot />
  </div>
</div>