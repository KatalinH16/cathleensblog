---
export interface Props {
  prev: any;
  next: any;
}

const { prev, next } = Astro.props;
---

<style lang="scss">
  footer {
    padding: 60px 20px;
    max-width: 760px;
    margin: 0 auto;
    display: flex;
    justify-content: flex-end;

    a {
      line-height: 1;
      padding: 20px;
      gap: 20px;
      margin: 0;
    }
  }
</style>

<footer>
  <a class="prev" href={prev}>Prev</a>
  <a class="next" href={next}>Next</a>
</footer>